{
  "name": "ticketing",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "test": "mocha -r ts-node/register src/test/**/*.spec.ts --exit --timeout 50000",
    "test:docker:up": "docker-compose -f integration/docker-compose.yml up -d",
    "test:docker:down": "docker-compose -f integration/docker-compose.yml down",
    "coverage": "yarn nyc --reporter lcov yarn test",
    "start": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
    "init:data": "ts-node ./src/init.ts",
    "db:pull": "npx prisma db pull",
    "db:push": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "3.10.0",
    "body-parser": "^1.19.2",
    "connect-timeout": "^1.9.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "express-promise-router": "^4.1.1",
    "express-validator": "^6.14.0",
    "graphql": "^15.3.0",
    "redis": "^4.0.4",
    "underscore": "^1.13.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/connect-timeout": "^0.0.36",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.19",
    "@types/sinon": "^10.0.11",
    "@types/underscore": "^1.11.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "prisma": "^3.10.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/test/**/*.ts",
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}